{
  "name": "payload-razorpay",
  "version": "0.1.5-alpha.0",
  "description": "A Payload CMS plugin that integrates Razorpay payment gateway for handling online payments and refunds",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "import": "./dist/exports/client.js",
      "types": "./dist/exports/client.d.ts",
      "default": "./dist/exports/client.js"
    },
    "./rsc": {
      "import": "./dist/exports/rsc.js",
      "types": "./dist/exports/rsc.d.ts",
      "default": "./dist/exports/rsc.js"
    },
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "types": "./dist/hooks/index.d.ts",
      "default": "./dist/hooks/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm copyfiles && pnpm build:types && pnpm build:swc",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc --strip-leading-paths",
    "build:types": "tsc --outDir dist --rootDir ./src",
    "clean": "rimraf {dist,*.tsbuildinfo}",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,json}\" dist/",
    "dev": "payload run ./dev/server.ts",
    "gen": "pnpm dev:generate-importmap && pnpm dev:generate-types",
    "dev:generate-importmap": "pnpm dev:payload generate:importmap",
    "dev:generate-types": "pnpm dev:payload generate:types",
    "dev:payload": "cross-env PAYLOAD_CONFIG_PATH=./dev/payload.config.ts payload",
    "lint": "eslint",
    "lint:fix": "eslint ./src --fix",
    "prepublishOnly": "pnpm clean && pnpm build",
    "test": "jest"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@payloadcms/db-mongodb": "^3.35.0",
    "@payloadcms/db-postgres": "^3.35.0",
    "@payloadcms/db-sqlite": "^3.35.0",
    "@payloadcms/eslint-config": "^3.28.0",
    "@payloadcms/next": "^3.35.0",
    "@payloadcms/richtext-lexical": "^3.35.0",
    "@payloadcms/ui": "^3.35.0",
    "@swc-node/register": "^1.10.10",
    "@swc/cli": "0.6.0",
    "@swc/jest": "^0.2.37",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "clsx": "^2.1.1",
    "copyfiles": "2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.24.0",
    "eslint-config-next": "^15.3.0",
    "graphql": "^16.10.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "next": "^15.3.0",
    "open": "^10.1.1",
    "payload": "^3.35.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "qs-esm": "7.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rimraf": "^6.0.1",
    "sharp": "^0.33.5",
    "sort-package-json": "^3.0.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/crazytweeks/payload-razorpay.git"
  },
  "keywords": [
    "payload",
    "cms",
    "plugin",
    "razorpay",
    "payments",
    "ecommerce"
  ],
  "registry": "https://registry.npmjs.org/",
  "dependencies": {
    "razorpay": "^2.9.6"
  },
  "author": "Bhuvan BM",
  "bugs": {
    "url": "https://github.com/crazytweeks/payload-razorpay/issues"
  },
  "homepage": "https://github.com/crazytweeks/payload-razorpay#readme",
  "trustedDependencies": [
    "@swc/core",
    "esbuild",
    "mongodb-memory-server",
    "sharp"
  ]
}
